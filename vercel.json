{
  "version": 2,
  "routes": [
    {
      "src": "/api/track-click",
      "dest": "/api/track-click.js"
    },
    {
      "src": "/api/index",
      "dest": "/api/index.mjs"
    }
  ],
  "functions": {
    "api/track-click.js": {
      "maxDuration": 60
    },
    "api/index.mjs": {
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "lib/*.js",
      "use": "@vercel/node"
    }
  ]
}
